    <style>
        .coming-soon-title {
            font-size: 3.5rem;
            font-weight: bold;
            letter-spacing: 2px;
            color: #222;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            display: inline-block;
            position: relative;
            animation: zoomIn 1s ease-out;
        }

        .dots span {
            display: inline-block;
            font-size: 3.5rem;
            color: #222;
            animation: bounce 1.2s infinite;
        }

        .dots span:nth-child(2) {
            animation-delay: 0.2s;
        }

        .dots span:nth-child(3) {
            animation-delay: 0.4s;
        }

        @keyframes bounce {
            0%, 80%, 100% {
                transform: translateY(0);
            }
            40% {
                transform: translateY(-10px);
            }
        }

        @keyframes zoomIn {
            from {
                transform: scale(0.8);
                opacity: 0;
            }
            to {
                transform: scale(1);
                opacity: 1;
            }
        }

        .coming-soon-section {
            min-height: 80vh;
            display: flex;
            align-items: center;
            justify-content: center;
            /* background: linear-gradient(135deg, #c400ff, #ff6b6b); */
            background-image: url(../public/images/favicon.png);
            position: relative;
            overflow: hidden;
        }

        .coming-soon-content {
            text-align: center;
            color: #fff;
            padding: 20px;
        }

        .coming-soon-content h1 {
            font-size: 4rem;
            margin-bottom: 20px;
        }

        .coming-soon-content p {
            font-size: 1.5rem;
            margin-bottom: 30px;
            max-width: 600px;
            margin-left: auto;
            margin-right: auto;
        }

        .coming-soon-content .btn {
            font-size: 1.2rem;
            padding: 10px 20px;
        }

        @media (max-width: 768px) {
            .coming-soon-content h1 {
                font-size: 2.5rem;
            }
            .coming-soon-content p {
                font-size: 1rem;
            }
        }
    </style>
</head>

